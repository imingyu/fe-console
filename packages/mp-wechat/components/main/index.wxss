.fc-hand {
  position: fixed;
  bottom: 100rpx;
  right: 100rpx;
  z-index: 500;
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
  background-color: #000;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  line-height: 100rpx;
}

.fc-hand.is-hide {
  display: none;
}

.fc-modal {
  position: fixed;
  top: 50%;
  left: 0;
  right: 0;
  bottom: 0;
  color: #5e5e5e;
  z-index: 1000;
}

.fc-modal.is-full {
  top: 0;
}

.fc-modal-body {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 70%;
  background-color: #fff;
  display: flex;
  flex-direction: column;
}

.fc-tabs-container {
  background: #f3f3f3;
  color: #333333;
  border-top: 1px solid #cccccc;
  border-bottom: 1px solid #cccccc;
  color: #5e5e5e;
  position: relative;
  display: flex;
  flex-wrap: nowrap;
}

.fc-tabs-scroll {
  width: 630rpx;
}

.fc-tabs-action {
  width: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  z-index: 5;
}

.fc-tabs-action::after {
  content: '';
  position: absolute;
  right: 0;
  top: 15rpx;
  width: 1rpx;
  height: 40rpx;
  background-color: #cccccc;
}

.fc-tabs-action-right::after {
  right: auto;
  left: 0;
}

.fc-tabs-zoom,
.fc-tabs-close {
  position: relative;
  width: 30rpx;
  height: 30rpx;
}

.fc-tabs-zoom:before, .fc-tabs-zoom:after,
.fc-tabs-zoom .fc-ic-arrow-1,
.fc-tabs-zoom .fc-ic-arrow-2,
.fc-tabs-close:before,
.fc-tabs-close:after,
.fc-tabs-close .fc-ic-arrow-1,
.fc-tabs-close .fc-ic-arrow-2 {
  content: '';
  box-sizing: border-box;
  position: absolute;
}

.fc-tabs-zoom.fc-ic-compress:before, .fc-tabs-zoom.fc-ic-compress:after,
.fc-tabs-zoom.fc-ic-compress .fc-ic-arrow-1,
.fc-tabs-zoom.fc-ic-compress .fc-ic-arrow-2, .fc-tabs-zoom.fc-ic-expand:before, .fc-tabs-zoom.fc-ic-expand:after,
.fc-tabs-zoom.fc-ic-expand .fc-ic-arrow-1,
.fc-tabs-zoom.fc-ic-expand .fc-ic-arrow-2 {
  width: 40%;
  height: 40%;
  border: 4rpx solid #5e5e5e;
}

.fc-tabs-zoom.fc-ic-compress:before, .fc-tabs-zoom.fc-ic-compress:after,
.fc-tabs-zoom.fc-ic-compress .fc-ic-arrow-1,
.fc-tabs-zoom.fc-ic-compress .fc-ic-arrow-2 {
  transform: rotate(180deg);
}

.fc-tabs-zoom.fc-ic-compress:before, .fc-tabs-zoom.fc-ic-expand:before {
  top: 0;
  left: 0;
  border-right-color: transparent;
  border-bottom-color: transparent;
}

.fc-tabs-zoom.fc-ic-compress:after, .fc-tabs-zoom.fc-ic-expand:after {
  right: 0;
  top: 0;
  border-left-color: transparent;
  border-bottom-color: transparent;
}

.fc-tabs-zoom.fc-ic-compress .fc-ic-arrow-1,
.fc-tabs-zoom.fc-ic-expand .fc-ic-arrow-1 {
  left: 0;
  bottom: 0;
  border-top-color: transparent;
  border-right-color: transparent;
}

.fc-tabs-zoom.fc-ic-compress .fc-ic-arrow-2,
.fc-tabs-zoom.fc-ic-expand .fc-ic-arrow-2 {
  right: 0;
  bottom: 0;
  border-top-color: transparent;
  border-left-color: transparent;
}

.fc-tabs-close.fc-ic-close:before, .fc-tabs-close.fc-ic-close:after {
  width: 100%;
  top: 50%;
  background-color: #5e5e5e;
  margin-top: -2rpx;
  height: 4rpx;
}

.fc-tabs-close.fc-ic-close:before {
  transform: rotate(45deg);
}

.fc-tabs-close.fc-ic-close:after {
  transform: rotate(135deg);
}

.fc-tabs-body {
  height: 100%;
  flex-shrink: 1;
}

.fc-tabs {
  display: flex;
  flex-wrap: nowrap;
  margin-bottom: -1px;
}

.fc-tab {
  box-sizing: border-box;
  flex-shrink: 0;
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 30rpx;
  text-align: center;
  border-bottom: 6rpx solid transparent;
  font-size: 28rpx;
  transition: border-color .5s;
}

.fc-tab.active {
  border-bottom-color: #1a73ea;
}

.fc-tabs-panels {
  display: flex;
  flex-wrap: nowrap;
  overflow: hidden;
  box-sizing: border-box;
  transition: transform .5s;
  height: 100%;
}

.fc-tabs-panels .fc-tabs-panel {
  flex-shrink: 0;
  flex: 1;
  box-sizing: border-box;
  padding: 20rpx 0;
  height: 100%;
  position: relative;
}

.fc-tabs-panels .tab-panel-reader {
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}

.fc-item {
  font-size: 30rpx;
  padding: 0 20rpx;
}

.fc-item.is-fold {
  padding-left: 42rpx;
}

.fc-item.is-fold::before {
  left: 20rpx;
}

.is-fold {
  padding-left: 22rpx;
  position: relative;
}

.is-fold:before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 12rpx;
  height: 12rpx;
  overflow: hidden;
  border: 12rpx solid transparent;
  border-left-color: #727272;
  box-sizing: border-box;
}

.is-fold.is-open {
  border-left-color: none;
  border-top-color: #727272;
}

.fc-item-view:hover {
  background-color: #ebf1fb;
}

.fc-item-view:before {
  top: 12rpx;
}

.fc-item-view .view-head,
.fc-item-view .view-body,
.fc-item-view .view-footer {
  display: inline-block;
}

.fc-item-view.is-open .view-head {
  background-color: #cfe8fc;
}

.fc-item-view.is-open .view-head,
.fc-item-view.is-open .view-body,
.fc-item-view.is-open .view-footer {
  display: block;
}

.fc-item-view.is-open .view-arrow.arrow-end {
  margin-left: 0;
}

.fc-item-view .view-arrow.arrow-end {
  margin-left: 14rpx;
}

.fc-item-view .view-part,
.fc-item-view .attr-part {
  display: inline-block;
}

.fc-item-view .view-arrow,
.fc-item-view .attr-equal,
.fc-item-view .attr-sem {
  color: #a995a7;
}

.fc-item-view .view-tag {
  color: #8c1d86;
}

.fc-item-view .view-attr {
  margin-left: 14rpx;
}

.fc-item-view .attr-name {
  color: #9a4804;
}

.fc-item-view .attr-value {
  color: #1a1aa6;
}

.fc-item-view .attr-value.attr-string {
  color: #c41a16;
}
