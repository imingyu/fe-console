<view class="fc-reader-row fc-renderer fc-rd-api sz-{{size}} cid-{{$cid}} tid-{{$tid}}">
    <view class="fc-col fc-api-col fc-col-1">
        <view class="f-title">{{data.name}}</view>
        <view v-if="{{data.desc}}" class="f-desc">{{data.desc}}</view>
    </view>
    <view class="fc-col fc-api-col fc-col-2">
        <view class="f-status">
            {{data.status===1?'Executed':data.status===2?'Success':data.status===3?'Fail':'Unknown'}}
        </view>
        <view v-if="{{data.statusDesc}}" class="f-desc">{{data.statusDesc}}</view>
    </view>
    <view class="fc-col fc-api-col fc-col-3">
        <view class="f-type">{{data.type}}</view>
    </view>
    <view class="fc-col fc-api-col fc-col-4">
        <!-- TODO:initiator -->
    </view>
    <view class="fc-col fc-api-col fc-col-5 is-last-col">
        <view class="f-time level-{{timeLevel}}">{{timeVal}}{{timeUnit}}</view>
    </view>
</view>