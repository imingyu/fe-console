{
    "name": "fe-console",
    "version": "1.0.0",
    "description": "小程序控制台辅助工具",
    "main": "index.js",
    "scripts": {
        "build": "node build/clear.js && tsc --emitDeclarationOnly && node --max-old-space-size=2048 build/build.js",
        "clear": "node build/clear.js",
        "dev": "node build/watch.js",
        "test": "mocha packages/**/test/*.js",
        "render:mp-wechat": "node build/render.js wechat",
        "tsc:mp-wechat": "npx tsc -b packages/mp-wechat",
        "fill:mp-wechat": "node build/fill.js wechat",
        "npm:mp-wechat": "npm run render:mp-wechat && npm run tsc:mp-wechat && npm run fill:mp-wechat && node build/demo.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/imingyu/mp-console.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/imingyu/mp-console/issues"
    },
    "homepage": "https://github.com/imingyu/mp-console#readme",
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-typescript": "^7.12.7",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^2.3.4",
        "@rollup/plugin-typescript": "^8.1.0",
        "autoprefixer": "^10.2.4",
        "babel-plugin-lodash": "^3.3.4",
        "chai": "^4.3.0",
        "fs-extra": "^9.1.0",
        "fse": "^4.0.1",
        "junk": "^3.1.0",
        "lerna": "^3.22.1",
        "lodash": "^4.17.21",
        "mocha": "^8.3.0",
        "node-sass": "^5.0.0",
        "postcss": "^8.2.6",
        "rimraf": "^3.0.2",
        "rollup": "^2.40.0",
        "rollup-plugin-uglify": "^6.0.4",
        "typescript": "^4.3.0-dev.20210228",
        "watch": "^1.0.2"
    },
    "dependencies": {
        "forgiving-xml-parser": "^1.3.0",
        "squirrel-report": "^1.0.2",
        "@mpkit/set-data": "^1.0.17",
        "@mpkit/mixin": "^1.0.17",
        "@mpkit/types": "^1.0.17",
        "@mpkit/mpxml-translator": "^1.0.17",
        "@mpkit/mpxml-parser": "^1.0.17"
    }
}
