<view class="fc-reader-row fc-nowrap {{isError?'fc-status-error':''}} fc-renderer fc-rd-api {{selected?'is-selected':''}} sz-{{size}} cid-{{$cid}} tid-{{$tid}} {{outerClass||''}}" bindtap="tap">
    <view class="fc-col fc-api-col fc-col-1">
        <view class="fc-text-primary">{{data.name}}</view>
        <view wx:if="{{data.desc}}" class="fc-text-muted">{{data.desc}}</view>
    </view>
    <view class="fc-col fc-api-col fc-col-2">
        <view class="fc-text-primary">
            {{data.statusCode?data.statusCode: (data.status===1?'Executed':data.status===2?'Success':data.status===3?'Fail':data.status)}}
        </view>
        <view wx:if="{{data.statusDesc}}" class="fc-text-muted">{{data.statusDesc}}</view>
    </view>
    <view class="fc-col fc-api-col fc-col-3">
        <view class="fc-text-primary">{{data.type}}</view>
    </view>
    <view class="fc-col fc-api-col fc-col-4">
        <view class="fc-text-primary" catchtap="tapInitiator">{{data.initiator}}</view>
        <view wx:if="{{data.initiatorDesc}}" class="fc-text-muted">{{data.initiatorDesc}}</view>
    </view>
    <view class="fc-col fc-api-col fc-col-right fc-col-5 is-last-col">
        <view class="fc-text-primary level-{{timeLevel}}">{{timeVal}}{{timeUnit}}</view>
    </view>
</view>